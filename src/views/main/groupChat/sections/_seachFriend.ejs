<% users.forEach(function(user) { %>
   <div data-uid="test-uid-01">
      <li data-uid="test-uid-01">
         <div class="contactPanel">
               <div class="user-avatar">
                  <img src="images/users/<%= user.avatar %>" alt="">
               </div>
               <div class="user-name"><p><%= user.username %></p></div>
               <br>
               <div class="user-address"><span>&nbsp;<%= user.address %></span></div>
               <div class="add-user" data-uid="test-uid-01">Thêm vào nhóm</div>
         </div>
      </li>
   </div>
<% });%>

