<div class="col-sm-9 sidenav no-padding">
	<div class="tab-content" id="screen-chat">
		<% allConversationWithMess.forEach((Conversation, i)=>{ %>
			<% if(Conversation.members){ %>
				<div class="right tab-pane <% if(i === 0){%> actice <%}%>" data-chat="<%= Conversation._id%>"
					id="to_<%= Conversation._id%>">
					<div class="top">
						<span>To: <span class="name"><%= Conversation.name%></span></span>
						<span class="chat-menu-right">
							<a href="#attachsModal_<%= Conversation._id%>" class="show-attachs" data-toggle="modal">
								Tệp đính kèm
								<i class="fa fa-paperclip"></i>
							</a>
						</span>
						<span class="chat-menu-right"><a href="javascript:void(0)">&nbsp;</a></span>
						<span class="chat-menu-right">
							<a href="#imagesModal_<%= Conversation._id %>" class="show-images" data-toggle="modal">Hình ảnh<i class="fa fa-photo"></i></a>
						</span>
						<span class="chat-menu-right">
							<a href="javascript:void(0)">&nbsp;</a>
						</span>
						<span class="chat-menu-right">
							<a href="javascript:void(0)" class="number-members" data-toggle="modal">
								<span class="show-number-members"><%= Conversation.usersAmout %></span>
								<i class="fa fa-users"></i>
							</a>
						</span>
						<span class="chat-menu-right">
							<a href="javascript:void(0)">&nbsp;</a>
						</span>
						<span class="chat-menu-right">
							<a href="javascript:void(0)" class="number-message" data-toggle="modal">
								<span class="show-number-message"><%= Conversation.messagesAmount %></span>
								<i class="fa fa-comment-o"></i>
							</a>
						</span>
					</div>
					<div class="content-chat">
						<div class="chat" data-chat="<%= Conversation._id%>">
							<% Conversation.message.forEach(function(mess){ %>
								<% if(mess.messageType === "text"){%>
									<div class="convert-emoji bubble <% if(mess.senderId == user._id){ %>me<%}else{%>you<%}%>"data-mess-id="<%= mess._id %>">
										<img src="/images/users/<%=mess.sender.avatar%>" class="avatar-small" title="<%= mess.sender.name%>">
										<%= mess.text %>
									</div>
								<% } %>
								<% if(mess.messageType === "image"){%>
									<div class="bubble <% if(mess.senderId == user._id){ %>me<%}else{%>you<%}%> bubble-image-file" data-mess-id="<%= mess._id %>">
										<img src="/images/users/<%=mess.sender.avatar%>" class="avatar-small" title="<%= mess.sender.name%>">
										<img src="data: <%=mess.file.contentType%>; base64, <%= bufferToBase64(mess.file.data)%>" class="show-image-chat" />
									</div>	
								<% } %>
								<% if(mess.messageType === "file"){%>
									<div class="bubble <% if(mess.senderId == user._id){ %>me<%}else{%>you<%}%> bubble-image-file" data-mess-id="<%= mess._id %>">
										<img src="/images/users/<%=mess.sender.avatar%>" class="avatar-small" title="<%= mess.sender.name%>">
										<a href="data: <%=mess.file.contentType%>; base64, <%= bufferToBase64(mess.file.data)%>" download="<%=mess.file.fileName%>"><%=mess.file.fileName%></a>
								</div>
								<% } %>
							<% }); %>
						</div>
					</div>
					<div class="write" data-chat="<%= Conversation._id%>">
						<input type="text" class="write-chat chat-in-group" id="write-chat-<%= Conversation._id%>" data-chat="<%= Conversation._id%>" />
						<div class="icons">
							<a href="#" class="icon-chat" data-chat="<%= Conversation._id%>"><i class="fa fa-smile-o"></i></a>
							<label for="image-chat-<%= Conversation._id %>">
								<input type="file" id="image-chat-<%= Conversation._id %>" name="my-image-chat" class="image-chat chat-in-group" data-chat="<%= Conversation._id%>" />
								<i class="fa fa-photo"></i>
							</label>
							<label for="attach-chat">
								<input type="file" id="attach-chat" name="my-attach-chat" class="attach-chat" data-chat="<%= Conversation._id%>" />
								<i class="fa fa-paperclip"></i>
							</label>
							<a href="#streamModal" id="video-chat" class="video-chat" data-chat="<%= Conversation._id%>" data-toggle="modal">
								<i class="fa fa-video-camera"></i>
							</a>
							<input type="hidden" id="peer-id" value="" />
						</div>
					</div>
				</div>
			<% } else{%>
				<div class="right tab-pane <% if(i === 0){%> actice <%}%>" data-chat="<%= Conversation._id%>" id="to_<%= Conversation._id%>">
					<div class="top">
						<span>To: <span class="name"><%= Conversation.username%></span></span>
						<span class="chat-menu-right">
							<a href="#attachsModal_<%= Conversation._id%>" class="show-attachs" data-toggle="modal">
								Tệp đính kèm
								<i class="fa fa-paperclip"></i>
							</a>
						</span>
						<span class="chat-menu-right">
							<a href="javascript:void(0)">&nbsp;</a>
						</span>
						<span class="chat-menu-right">
							<a href="#imagesModal_<%= Conversation._id %>" class="show-images" data-toggle="modal">
								Hình ảnh
								<i class="fa fa-photo"></i>
							</a>
						</span>
					</div>
					<div class="content-chat">
						<div class="chat" data-chat="<%= Conversation._id%>">
							<% Conversation.message.forEach((mess)=>{ %>
								<% if(mess.messageType === "text"){%>
									<div class="convert-emoji bubble <% if(mess.senderId == user._id){ %>me<%}else{%>you<%}%>"data-mess-id="<%= mess._id %>">
										<img src="/images/users/<%=mess.sender.avatar%>" class="avatar-small" title="<%= mess.sender.name%>" title="">
										<%= mess.text %>
									</div>
								<% } %>
								<% if(mess.messageType === "image"){%>
									<div class="bubble <% if(mess.senderId == user._id){ %>me<%}else{%>you<%}%> bubble-image-file" data-mess-id="<%= mess._id %>">
										<img src="/images/users/<%=mess.sender.avatar%>" class="avatar-small" title="<%= mess.sender.name%>">
										<img src="data: <%=mess.file.contentType%>; base64, <%= bufferToBase64(mess.file.data)%>" class="show-image-chat" />
									</div>	
								<% } %>
								<% if(mess.messageType === "file"){%>
									<div class="bubble <% if(mess.senderId == user._id){ %>me<%}else{%>you<%}%> bubble-image-file" data-mess-id="<%= mess._id %> " data-mess-id="<%= mess._id %>">
										<img src="/images/users/<%=mess.sender.avatar%>" class="avatar-small" title="<%= mess.sender.name%>">
										<a href="data: <%=mess.file.contentType%>; base64, <%= bufferToBase64(mess.file.data)%>" download="<%=mess.file.fileName%>"><%=mess.file.fileName%></a>
								</div>
								<% } %>
							<% }); %>
						</div>
					</div>
					<div class="write" data-chat="<%= Conversation._id%>">
						<input type="text" class="write-chat" id="write-chat-<%= Conversation._id%>" data-chat="<%= Conversation._id%>" />
						<div class="icons">
							<a href="#" class="icon-chat" data-chat="<%= Conversation._id%>"><i class="fa fa-smile-o"></i></a>
							<label for="image-chat-<%= Conversation._id %>">
								<input type="file" id="image-chat-<%= Conversation._id %>" name="my-image-chat" class="image-chat" data-chat="<%= Conversation._id%>">
								<i class="fa fa-photo"></i>
							</label>
							<label for="attach-chat">
								<input type="file" id="attach-chat" name="my-attach-chat" class="attach-chat" data-chat="<%= Conversation._id%>" />
								<i class="fa fa-paperclip"></i>
							</label>
							<a href="#streamModal" id="video-chat" class="video-chat" data-chat="<%= Conversation._id%>" data-toggle="modal">
								<i class="fa fa-video-camera"></i>
							</a>
							<input type="hidden" id="peer-id" value="" />
						</div>
					</div>
				</div>
			<% } %>
		<% }); %>
	</div>
</div>